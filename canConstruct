bool canConstruct(char * ransomNote, char * magazine){
    int count[26]={0};//统计字符串中包含的26个英文字母的分布情况
    while(*magazine)
    {
        count[*magazine - 'a'] += 1;
        magazine++;
    }
    //遍历ransomNote字符串中的所有字符，是否都在count数组中存在多个
    while(*ransomNote)
    {
        if(count[*ransomNote - 'a'] <= 0)
            return false;
        else
        {
            count[*ransomNote - 'a'] -= 1;
            ransomNote++;
        }
    }
    return true;
}
